{
  "name": "libtuvero",
  "version": "1.6.0-dev",
  "description": "libtuvero",
  "main": "./dist/index",
  "author": {
    "name": "Erik E. Lorenz"
  },
  "scripts": {
    "build": "tsc",
    "build-watch": "tsc -w",
    "bundle": "webpack",
    "bundle-watch": "webpack --watch",
    "lint": "tslint --config tslint.json --project tsconfig.json",
    "lint-fix": "tslint --config tslint.json --project tsconfig.json --fix",
    "test": "mocha dist/**/*.spec.js",
    "test-src": "mocha -r node_modules/ts-node/register src/**/*.spec.ts",
    "coverage": "nyc npm test"
  },
  "dependencies": {
    "@types/lodash": "4.14.104",
    "@types/mathjs": "0.0.37",
    "lodash": "4.17.5",
    "mathjs": "3.20.1",
    "mersenne": "0.0.4"
  },
  "devDependencies": {
    "@types/chai": "4.1.2",
    "@types/mocha": "2.2.48",
    "chai": "4.1.2",
    "mocha": "5.0.1",
    "nyc": "11.4.1",
    "uglifyjs-webpack-plugin": "1.2.0",
    "ts-node": "4.1.0",
    "tslint": "5.9.1",
    "typescript": "2.7.2",
    "webpack": "3.11.0"
  },
  "nyc": {
    "all": true,
    "include": "dist/**/*.js",
    "exclude": [
      "dist/**/*.spec.js",
      "dist/bundle*.js",
      "dist/index.js"
    ]
  }
}
